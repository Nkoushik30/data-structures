#include<stdio.h>
int main()
{
  int  i,n,a[10],b[10],j;
  
   printf("enter the value of n:");
   scanf("%d",&n);
   
  printf("enter values of array :");
  
  for(i=0;i<n;i++)
  {
  scanf("%d ",&a[i]);
  }
     for(i=n-1,j=0;i<=n,j<n;i--,j++)
     {
b[j]=a[i];
printf("%d ",b[j]);  
     
     }
     
     return 0;
     }
    
